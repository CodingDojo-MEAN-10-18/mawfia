<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <title>Cats Page</title>
        <link rel="stylesheet" type="text/css" href="css/cats.css">
        <!--[if lt IE 9]>
        <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
    </head>
    <body>
        <h1>Some Really Cool Cats!</h1>
        <a href="/">Home</a>
        <div id="cats">
        <% for (var cat in cats) { %><a href="/<%= cats[cat].name %>"><img src="images/<%= cats[cat].picture %>"></a><% } %>
        </div>
    </body>
</html>
